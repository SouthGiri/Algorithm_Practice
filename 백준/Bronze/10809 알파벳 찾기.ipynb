   "source": [
    "# set 으로 바꾼 후 그 알파벳만 find 해서 입력\n",
    "\n",
    "s = input()\n",
    "ans = [-1] * 26\n",
    "ss = set(s)\n",
    "for i in ss:\n",
    "    ans[ord(i)-97] = s.find(i)\n",
    "\n",
    "for i in range(26):\n",
    "    print(ans[i], end=' ')"
   ]
  }
