    "import math\n",
    "\n",
    "n = int(input())\n",
    "num = str(math.factorial(n))\n",
    "cnt = 0\n",
    "\n",
    "for i in range(len(num)-1, 0, -1):\n",
    "    if num[i] == '0':\n",
    "        cnt += 1\n",
    "    else:\n",
    "        break\n",
    "print(cnt)"
   ]
  }
